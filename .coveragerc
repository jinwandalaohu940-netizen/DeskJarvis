[run]
source = agent
branch = True
omit =
    agent/executor/*
    agent/crew/*
    agent/main.py
    agent/server.py
    agent/planner/*
    agent/scheduler.py
    agent/workflows.py
    agent/history.py
    agent/user_input.py
    agent/memory/advanced_memory.py
    agent/memory/memory_manager.py
    agent/memory/vector_memory.py
    agent/tools/logger.py
    agent/tools/async_wrapper.py
    agent/tools/log_sanitizer.py
    agent/tools/memory_queue.py
    agent/tools/path_validator.py
    agent/tools/security_auditor.py

[report]
skip_empty = True
show_missing = True
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:

